{
    "name": "dainsys/clear-log-files",
    "description": "Clear Laravel log files",
    "type": "composer-plugin",
    "require": {
        "php" : ">=7.0",
        "illuminate/filesystem": "~5.7|~5.8",
        "illuminate/console": "~5.7|~5.8",
        "illuminate/support": "~5.7|~5.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yjorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\Commands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Commands\\ClearLogsServiceProvider"
            ]
        }
    }
}
