{
    "name": "dainsys/clear-laravel-logs",
    "description": "Clear Laravel log files",
    "require": {
        "php" : ">=7.0",
        "illuminate/filesystem": ">=5.7",
        "illuminate/console": ">=5.7",
        "illuminate/support": ">=5.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": ">=3.7",
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yjorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\Commands\\ClearLogs\\": "src/"
        }
    },
    "autoload-dev":  {
        "psr-4": {
            "Dainsys\\Commands\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Commands\\ClearLogs\\ClearLogsServiceProvider"
            ]
        }
    }
}
